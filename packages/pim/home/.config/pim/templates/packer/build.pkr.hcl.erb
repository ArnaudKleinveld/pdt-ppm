# Generated by pim packer on <%= template_run_date %>

packer {
  required_plugins {
<% plugins.each do |name, config| -%>
    <%= name %> = {
<% config.each do |key, value| -%>
      <%= key.to_s.ljust(7) %> = "<%= value %>"
<% end -%>
    }
<% end -%>
  }
}

build {
  sources = [
    "source.<%= builder.type %>.generic"
  ]

<%= provisioners_content %>

}
