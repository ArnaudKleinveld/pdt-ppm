# Generated by pim packer on <%= template_run_date %>

<% build_vars.sort.each do |key, value| -%>
<% formatted_value = case value
   when true, false then value.to_s
   when Integer, Float then value.to_s
   when Array then "[#{value.map { |v| v.is_a?(String) ? %Q("#{v}") : v.to_s }.join(', ')}]"
   when Hash then value.to_json
   when nil then 'null'
   else %Q("#{value.to_s.gsub('\\', '\\\\\\\\').gsub('"', '\\"')}")
   end
-%>
<%= key.to_s.ljust(40) %> = <%= formatted_value %>
<% end -%>
